{
  "name": "surfcontest_mobx",
  "version": "1.0.0",
  "description": "Surf Contest App using React, Webpack, Mobx",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && webpack --display-error-details",
    "clean": "rm -rf ./dist/*",
    "index": "cp ./public/index.html ./dist/",
    "dev": "webpack-dev-server --display-error-details --progress --colors --inline --hot",
    "deploy:gh": "gh-pages -d dist",
    "start": "nodemon --debug --exec babel-node server.js --presets es2015,stage-0",
    "server": "babel-node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ottoinfo/surfcontest-mobx.git"
  },
  "keywords": [
    "React",
    "Webpack",
    "Mobx"
  ],
  "author": "Matthew Otto <matt@ottoinfo.com> (http://ottoinfo.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ottoinfo/surfcontest-mobx/issues"
  },
  "homepage": "https://github.com/ottoinfo/surfcontest-mobx#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
